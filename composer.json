{
    "name": "openthinklabs/mailbox-component",
    "description": "The Mailbox component provides tools that allows your helpdesk to process tickets from multiple email sources.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "akshay kumar",
            "email": "akshay.kumar758@webkul.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/core-framework.git"
        }
   ],
   "require": {
        "php": "^7.3 || ^8.0",
        "uvdesk/composer-plugin": "^1.0",
        "openthinklabs/core-framework": "^1.0",
        "iamcal/lib_autolink": "^1.7",
        "php-mime-mail-parser/php-mime-mail-parser": "^7.0.0"
    },
    "autoload": {
        "psr-4": { "Webkul\\UVDesk\\MailboxBundle\\": "" }
    },
    "extra": {
        "uvdesk-package-extension": "Webkul\\UVDesk\\MailboxBundle\\Package\\Composer",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
